pipeline {
    agent any

    stages {
        stage('Build') {
            steps {
              sh 'cd my-app && mvn build'
            }
        }
        stage('Test'){
            steps{
            	mvn test
          		echo 'hello from myapp automated!!'
            }
        }
        stage('Publish'){
            steps {
               echo 'great success'
            }
        }
    }
}
